<link rel="stylesheet" href="/assets/css/style.css" />
<div class="jumbotron peach-gradient">
  <h2 class="h1-responsive font-weight-bold text-center my-4 white-text">Communities</h2>
</div>

<div class="container mt-5">
  {{#each communities}}
  <a class="search-card-link" href="/sites/{{this.GeoName}}/{{this.name}}">
    <div class="search-card card mx-auto">
      <div class="card-body">
        <h5 class="card-title">{{this.name}}</h5>
      </div>
    </div>
  </a>
  {{/each}}
</div>