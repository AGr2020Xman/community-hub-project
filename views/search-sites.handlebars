<link rel="stylesheet" href="/assets/css/createmypage.css">

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h2 class="h1-responsive my-4">Search results</h2>
        </div>
    </div>
</div>
<div class="container">
    {{#each sites}}
    <a class="search-card-link" href="/sites/{{geo}}/{{community}}/{{name}}">
        <div class="search-card card mx-auto">
            <div class="card-body">
                <h5 class="card-title">{{title}}</h5>
            </div>
        </div>
    </a>
    {{/each}}

</div>